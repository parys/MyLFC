{
  "name": "asp.net",
  "version": "1.0.0",
  "scripts": {
    "run": "ng serve --hmr false --proxy-config proxy.conf.json",
    "run2": "ng serve  --proxy-config proxy.conf.json",
    "build": "ng build --prod --build-optimizer",
    "build-ssr": "ng build --prod --project=ssr",
    "build-nohash": "ng build --prod --build-optimizer --output-hashing none",
    "build:ssr": "npm run build-ssr",
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/aspnetcorespa/stats.json",
    "analyze-es5": "webpack-bundle-analyzer dist/aspnetcorespa/stats-es5.json",
    "banalyze-es5": "npm run build -- --stats-json && webpack-bundle-analyzer dist/aspnetcorespa/stats-es5.json",
    "analyze-es2015": "webpack-bundle-analyzer dist/aspnetcorespa/stats-es2015.json",
    "banalyze-es2015": "npm run build -- --stats-json && webpack-bundle-analyzer dist/aspnetcorespa/stats-es2015.json",
    "source-map": "npm run build -- --sourceMap=true --outputHashing=none && source-map-explorer dist/aspnetcorespa/main.js dist/aspnetcorespa/main.js.map"
  },
  "dependencies": {
    "@angular/animations": "8.2.0",
    "@angular/cdk": "8.1.2",
    "@angular/common": "8.2.0",
    "@angular/compiler": "8.2.0",
    "@angular/compiler-cli": "8.2.0",
    "@angular/core": "8.2.0",
    "@angular/forms": "8.2.0",
    "@angular/material": "8.1.2",
    "@angular/platform-browser": "8.2.0",
    "@angular/platform-browser-dynamic": "8.2.0",
    "@angular/platform-server": "8.2.0",
    "@angular/router": "8.2.0",
    "@angular/service-worker": "8.2.0",
    "@aspnet/signalr": "1.0.4",
    "@aspnet/signalr-protocol-msgpack": "1.0.4",
    "@nguniversal/aspnetcore-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "angular2-recaptcha": "1.1.0",
    "aspnet-prerendering": "3.0.1",
    "aspnet-webpack": "3.0.0",
    "core-js": "3.1.4",
    "hammerjs": "2.0.8",
    "node-sass": "4.12.0",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.10.0",
    "ngx-image-cropper": "1.4.1",
    "ngx-pagination": "4.1.0"
  },
  "devDependencies": {
    "@angular/cli": "8.2.0",
    "@angular-devkit/build-angular": "0.802.0",
    "@angular/pwa": "0.802.0",
    "typescript": "3.5.3",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "codelyzer": "5.1.0",
    "tinymce": "4.9.3",
    "@types/node": "12.6.8",
    "@types/tinymce": "4.5.22",
    "webpack-bundle-analyzer": "3.4.1",
    "source-map-explorer": "2.0.1"
  },
  "main": "package.json",
  "author": "Andrei Kapitanchik",
  "license": "ISC",
  "description": ""
}
