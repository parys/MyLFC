{
  "name": "asp.net",
  "version": "1.0.0",
  "scripts": {
    "run": "ng serve --hmr false --proxy-config proxy.conf.json",
    "run2": "ng serve  --proxy-config proxy.conf.json",
    "build": "ng build --prod --build-optimizer",
    "build-ssr": "ng build --prod --project=ssr",
    "build-nohash": "ng build --prod --build-optimizer --output-hashing none",
    "build:ssr": "npm run build-ssr",
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/aspnetcorespa/stats.json",
    "source-map": "npm run build -- --sourceMap=true --outputHashing=none && source-map-explorer dist/aspnetcorespa/main.js dist/aspnetcorespa/main.js.map"
  },
  "dependencies": {
    "@angular/animations": "7.2.15",
    "@angular/cdk": "7.3.7",
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/compiler-cli": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/forms": "7.2.15",
    "@angular/http": "7.2.15",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/platform-server": "7.2.15",
    "@angular/router": "7.2.15",
    "@angular/service-worker": "7.2.15",
    "@aspnet/signalr": "1.0.4",
    "@aspnet/signalr-protocol-msgpack": "1.0.4",
    "@nguniversal/aspnetcore-engine": "7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "angular2-recaptcha": "1.1.0",
    "aspnet-prerendering": "3.0.1",
    "aspnet-webpack": "3.0.0",
    "core-js": "2.6.5",
    "hammerjs": "2.0.8",
    "ngx-image-cropper": "1.3.9",
    "ngx-pagination": "3.2.1",
    "node-sass": "4.12.0",
    "preboot": "7.0.0",
    "rxjs": "6.5.1",
    "swagger-ui": "3.22.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular/cli": "7.3.9",
    "@angular-devkit/build-angular": "0.13.9",
    "@angular/pwa": "0.13.9",
    "typescript": "3.4.5",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "codelyzer": "5.0.1",
    "tinymce": "4.9.3",
    "@types/node": "11.13.10",
    "@types/tinymce": "4.5.20",
    "webpack-bundle-analyzer": "3.3.2",
    "acorn": "6.1.1",
    "source-map-explorer": "1.8.0"
  },
